package array_flatten;

import java.util.Arrays;
import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] box = new int[100];
		for (int tc = 1; tc <= 10; tc++) {

			int dump = sc.nextInt();

			for (int i = 0; i < 100; i++)
				box[i] = sc.nextInt();

			Arrays.sort(box);

			while (dump-- > 0) {
				box[0]++;
				box[99]--;
			}

			Arrays.sort(box);

			System.out.println("#" + tc + " " + );
		}
	}
}
