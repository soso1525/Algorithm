package array_flatten;

import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] box = new int[100];
		for (int tc = 1; tc <= 10; tc++) {
			int ans = 0;
			
			for(int i=0; i<100; i++)
				box[i] = sc.nextInt();

			System.out.println("#" + tc + " " + ans);
		}
	}
}
