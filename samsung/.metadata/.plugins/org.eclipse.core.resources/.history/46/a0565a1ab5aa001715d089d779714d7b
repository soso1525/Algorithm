package array1;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int tc = 10;
		while (tc-- > 0) {
			int n = sc.nextInt();
			int[] a = new int[n];
			for (int i = 0; i < n; i++) {
				a[i] = sc.nextInt();
			}

			int[] tmp = new int[5];
			
			for (int i = 2; i < n - 2; i++) {
				if(a[i-2] < a[i] && a[i-1] < a[i] && a[i+1] > a[i] && a[i+2]) {
					tmp[0] = a[i-2];
					tmp[1] = a[i-1];
					tmp[2] = a[i];
					tmp[3] = a[i+1];
					tmp[4] = a[i+2];
					
				}
			}
		}

	}
}
