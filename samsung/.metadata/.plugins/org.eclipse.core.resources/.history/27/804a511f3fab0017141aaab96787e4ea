package array_maxsum;

import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int tc = 10;
		int n = 100;
		int[][] arr = new int[n][n];
		while (tc-- > 0) {

			int ans = 0;

			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					arr[i][j] = sc.nextInt();
				}
			}

			for (int i = 0; i < 100; i++) {
				int sum = 0;
				for (int num : arr[i])
					sum += num;

				if (ans < sum)
					ans = sum;
			}
		}
	}
}
