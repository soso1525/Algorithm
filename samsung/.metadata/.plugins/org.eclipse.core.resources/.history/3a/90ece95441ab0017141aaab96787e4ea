package array_maxsum;

public class Main {
	public static void main(String[] args) {
		int n = 5;
		int[][] arr = new int[n][n];

		int k = 1;

		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				arr[i][j] = k++;
			}
		}

		// for (int i = 0; i < n; i++) {
		// for (int j = 0; j < n; j++) {
		// System.out.print(arr[i][j] + "\t");
		// }
		// System.out.println();
		// }

		for (int i = 0; i < n; i++) {
			System.out.print(arr[i][n-i-1]);
		}
	}
}
