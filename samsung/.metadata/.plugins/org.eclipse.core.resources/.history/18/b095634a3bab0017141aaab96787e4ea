package partial_set;

public class Main {
	public static void main(String[] args) {

		int n = 4;
		int[] bit = new int[n];

		for (int i = 0; i < 2; i++) {
			if (i % 2 == 0)
				bit[0] = 0;
			else
				bit[0] = 1;

			for (int j = 0; j < 2; j++) {
				if (j % 2 == 0)
					bit[1] = 0;
				else
					bit[1] = 1;

				for (int k = 0; k < 2; k++) {
					if (k % 2 == 0)
						bit[2] = 0;
					else
						bit[2] = 1;
					
					for (int l= 0; l < 2; l++) {
						if (l % 2 == 0)
							bit[0] = 0;
						else
							bit[0] = 1;
						
						
					}
				}
			}
		}
	}
}
