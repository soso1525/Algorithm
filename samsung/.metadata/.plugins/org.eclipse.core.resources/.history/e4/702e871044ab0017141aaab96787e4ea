package array_maxsum;

import java.util.ArrayList;
import java.util.Collections;
import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = 100;
		int[][] arr = new int[n][n];

		for (int tc = 1; tc <= 10; tc++) {
			// arr initialization
			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					arr[i][j] = sc.nextInt();
				}
			}

			int ans = -1;

			for (int i = 0; i < n; i++) {
				int tmp1 = 0, tmp2 = 0;
				for (int j = 0; j < n; j++) {
					tmp1 += arr[i][j];
					tmp2 += arr[j][i];
				}

				if (ans < tmp1)
					ans = tmp1;
				if (ans < tmp2)
					ans = tmp2;
			}

			for (int i = 0; i < n; i++) {
				int tmp1 = 0, tmp2 = 0;
				tmp1 += arr[i][i];
				tmp2 += arr[i][n - i - 1];
			}

			Collections.sort(al);
			Collections.reverse(al);

			System.out.println("#" + tc + " " + al.get(0));
		}
	}
}
