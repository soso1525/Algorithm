package array_maxsum;

import java.util.ArrayList;
import java.util.Collections;
import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int tc = 10;
		int n = 100;
		int[][] arr = new int[n][n];

		while (tc-- > 0) {
			ArrayList<Integer> al = new ArrayList<Integer>();
			int ans = 0, tmp11 = 0, tmp12 = 0;

			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					arr[i][j] = sc.nextInt();
				}
			}

			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					tmp11 += arr[i][j];
				}
				al.add(tmp1);
				tmp1 = 0;
			}

			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					tmp1 += arr[j][i];
				}
				al.add(tmp1);
				tmp1 = 0;
			}

			for (int i = 0; i < n; i++)
				tmp1 += arr[i][i];
			al.add(tmp1);
			tmp1 = 0;

			for (int i = 0; i <a n; i++)
				tmp1 += arr[i][n - i - 1];
			al.add(tmp1);

			Collections.sort(al);
			System.out.println(al.get(al.size() - 1));
		}
	}
}
