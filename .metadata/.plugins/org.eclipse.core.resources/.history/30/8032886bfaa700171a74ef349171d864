import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();

		// (a + b) % c
		System.out.println((a + b) % c);

		// (a%c + b%c) % c
		// (a * b) % c
		// (a%c * b%c) % c
	}
}
