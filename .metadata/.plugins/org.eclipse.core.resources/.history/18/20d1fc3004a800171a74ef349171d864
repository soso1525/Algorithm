import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int b = sc.nextInt();

		StringBuilder sb = new StringBuilder();

		while (n / b != 0) {
			int r = n % b;
			if (r > 9) {
				sb.append((char) (r + 55));
			}

		}

	}
}
